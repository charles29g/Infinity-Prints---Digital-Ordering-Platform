@{
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<body>
    <div class="sidebar">
        <h1>InfinityPrintz<br> Admin Panel</h1>
        <ul>
            <li style="margin-bottom: 5px;">
                <a ng-href="/Home/DashAccount">
                    <span style="display: inline-block; width: 20px; text-align: center; margin-right: 10px;">
                        <i class="fas fa-user-circle" style="font-size: 18px;"></i>
                    </span>
                    Account
                </a>
            </li>
            <li style="margin-bottom: 5px;">
                <a ng-href="/Home/DashBooking">
                    <span style="display: inline-block; width: 20px; text-align: center; margin-right: 10px;">
                        <i class="fas fa-star" style="font-size: 18px;"></i>
                    </span>
                    Reviews
                </a>
            </li>

            <li style="margin-bottom: 5px;">
                <a ng-href="/Home/DashService">
                    <span style="display: inline-block; width: 20px; text-align: center; margin-right: 10px;">
                        <i class="fas fa-shopping-cart" style="font-size: 18px;"></i>
                    </span>
                    Services
                </a>
            </li>
            <li style="margin-bottom: 5px;">
                <a ng-href="/Home/DashOrders">
                    <span style="display: inline-block; width: 20px; text-align: center; margin-right: 10px;">
                        <i class="fas fa-box-open" style="font-size: 18px;"></i>
                    </span>
                    Orders
                </a>
            </li>
            <li style="margin-bottom: 5px;">
                <a ng-href="/Home/DashContent">
                    <span style="display: inline-block; width: 20px; text-align: center; margin-right: 10px;">
                        <i class="fas fa-file-alt" style="font-size: 18px;"></i>
                    </span>
                    Content
                </a>
            </li>
            <li style="margin-bottom: 5px;" class="active">
                <a ng-href="/Home/DashUsers">
                    <span style="display: inline-block; width: 20px; text-align: center; margin-right: 10px;">
                        <i class="fas fa-users active" style="font-size: 18px;"></i>
                    </span>
                    Users
                </a>
            </li>
            <li style="margin-bottom: 5px;">
                <a ng-href="/Home/DashPayments">
                    <span style="display: inline-block; width: 20px; text-align: center; margin-right: 10px;">
                        <i class="fas fa-credit-card" style="font-size: 18px;"></i>
                    </span>
                    Payment
                </a>
            </li>
            <li style="margin-bottom: 5px;">
                <a ng-href="/Home/DashLog">
                    <span style="display: inline-block; width: 20px; text-align: center; margin-right: 10px;">
                        <i class="fas fa-comments" style="font-size: 18px;"></i>
                    </span>
                    Chat
                </a>
            </li>
            <li style="margin-bottom: 5px;">
                <a ng-href="/Home/DashUser">
                    <span style="display: inline-block; width: 20px; text-align: center; margin-right: 10px;">
                        <i class="fas fa-book-open" style="font-size: 18px;"></i>
                    </span>
                    User Guide
                </a>
            </li>
            <li style="margin-bottom: 5px;">
                <a ng-href="/Home/DashStatus">
                    <span style="display: inline-block; width: 20px; text-align: center; margin-right: 10px;">
                        <i class="fas fa-spinner active" style="font-size: 18px;"></i>
                    </span>
                    Status
                </a>
            </li>
            <li style="margin-bottom: 5px;">
                <a ng-href="/Home/DashReceipt">
                    <span style="display: inline-block; width: 20px; text-align: center; margin-right: 10px;">
                        <i class="fas fa-receipt" style="font-size: 18px;"></i>
                    </span>
                    Receipts
                </a>
            </li>
        </ul>
        <div class="sidebar-footer">
            <div class="user-info">
                <button class="logout-btn" ng-click="logoutfunc()">
                    <p class="user-name"><i class="fas fa-user-circle"></i> {{ getFullName }}</p>
                    <i class="fas fa-sign-out-alt"></i>
                </button>

            </div>
        </div>
    </div>

    <div class="main-content">
        <p style="text-align: right;">Employee Dashboard</p>
        <h1 style="font-size: 2.5rem; font-weight: 600; color: #6f0d4a;  text-align: center;">
            Logs
        </h1>
        <div class="row white-text m2" ng-init="loadLogs()">

            <div class="table-wrapper">
                <table id="myTable" class="display">
                    <thead>
                        <tr>
                            <th>Log ID</th>
                            <th>User ID</th>
                            <th>Action</th>
                            <th>Created At</th>
                            <th>Delete</th>

                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="pDATA in LogsData">
                            <td>{{pDATA.LogID}}</td>

                            <td>{{pDATA.UserID}}</td>
                            <td>{{pDATA.Action}}</td>
                            <td>{{DateFormat(pDATA.CreatedAt)}}</td>
                            <td></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</body>
